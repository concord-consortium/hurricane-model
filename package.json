{
  "name": "hurricane-model",
  "version": "1.5.3",
  "description": "Concord Consortium Hurricane Model",
  "main": "index.js",
  "jest": {
    "testURL": "https://hurricane-model.unexisting.url.com",
    "setupFilesAfterEnv": [
      "jest-canvas-mock",
      "<rootDir>src/setupTests.js"
    ],
    "transform": {
      ".(ts|tsx|js)$": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/test-utils.ts"
    ],
    "moduleNameMapper": {
      "\\.svg$": "<rootDir>/__mocks__/svgMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --hot",
    "build": "npm-run-all lint:build clean build:webpack",
    "build:webpack": "webpack --mode production --devtool false",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:build": "tslint -c tslint-build.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint-build.json --fix 'src/**/*.{ts,tsx}'",
    "lint:unused": "tsc --noUnusedLocals --project .",
    "test": "jest",
    "test:all": "npm-run-all build test start",
    "test:coverage": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache",
    "test:watch": "jest --watch",
    "test:coverage:watch": "jest --coverage --watchAll",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:full": "npm-run-all test test:cypress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/hurricane-model.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/hurricane-model/issues"
  },
  "homepage": "https://github.com/concord-consortium/hurricane-model#readme",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@svgr/webpack": "^5.5.0",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/istanbul-lib-coverage": "^2.0.3",
    "@types/jest": "^27.0.2",
    "@types/leaflet": "^1.7.5",
    "@types/pixi.js": "^4.8.9",
    "@types/pngjs": "^3.3.2",
    "@types/react": "^16.14.17",
    "@types/react-dom": "^16.9.14",
    "@types/react-jsonschema-form": "^1.7.7",
    "@types/react-leaflet": "^2.8.2",
    "@types/seedrandom": "^3.0.1",
    "autoprefixer": "^10.3.7",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "^5.2.7",
    "cypress": "^13.7.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^4.5.2",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-lib-coverage": "^3.2.0",
    "jest": "^27.3.1",
    "jest-canvas-mock": "^2.3.1",
    "jest-fetch-mock": "^3.0.3",
    "mercator-projection": "0.0.2",
    "mini-css-extract-plugin": "^0.12.0",
    "node-pre-gyp": "^0.17.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.9",
    "postcss-loader": "^4.3.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.59.3",
    "sass-loader": "^10.2.0",
    "script-loader": "^0.7.2",
    "style-loader": "^1.3.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.3.0",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.2.0",
    "typescript": "^4.4.4",
    "url-loader": "^2.3.0",
    "wait-on": "^6.0.0",
    "webpack": "^4.47.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "@concord-consortium/lara-interactive-api": "^1.1.2",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@turf/helpers": "^6.5.0",
    "@turf/line-intersect": "^6.5.0",
    "d3-scale": "^3.3.0",
    "geolocation-utils": "1.2.5",
    "jquery": "^3.6.0",
    "kd-tree-javascript": "^1.0.3",
    "leaflet": "^1.7.1",
    "mobx": "^5.15.7",
    "mobx-react": "^5.4.4",
    "pixi.js": "^4.8.9",
    "pngjs": "3.3.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-jsonschema-form": "^1.8.1",
    "react-leaflet": "^2.8.0",
    "react-leaflet-control": "^2.1.2",
    "screenfull": "^4.2.1",
    "seedrandom": "^3.0.5",
    "shutterbug": "^1.3.2"
  }
}
